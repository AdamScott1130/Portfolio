						{
							"name": "Create Driver",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"pm.test(\"POST request to /drivers for creating a new driver returned code '201 OK'\", () => {\r",
											"    pm.response.to.have.status(201);\r",
											"});\r",
											"\r",
											"const responseJson = pm.response.json();\r",
											"\r",
											"pm.test(\"City ID in the response body matches City ID in the /drivers POST request body.\", () => {\r",
											"    pm.expect(responseJson.cityId).to.match(/1/);\r",
											"});\r",
											"\r",
											"pm.test(\"Latitude in response body matches latitude in the /drivers POST request body.\", () => {\r",
											"    pm.expect(responseJson.lastKnownLatitude).to.match(/43° 28' 39.33''/);\r",
											"});\r",
											"\r",
											"pm.test(\"Longitude in response body matches longitude in the /drivers POST request body.\", () => {\r",
											"    pm.expect(responseJson.lastKnownLongitude).to.match(/80° 31' 32.12''/);\r",
											"});\r",
											"\r",
											"pm.test(\"isActive in response body matches isActive in the /drivers POST request body.\", () => {\r",
											"    pm.expect(responseJson.isActive).to.equal(true);\r",
											"});\r",
											"\r",
											"pm.test(\"Driver created by POST request to /drivers has a driverId'\", () => {\r",
											"    pm.expect(responseJson).to.have.property(\"driverId\");\r",
											"    pm.collectionVariables.set(\"testDriverId\", responseJson.driverId);\r",
											"    pm.collectionVariables.set(\"testBadDriverId\", responseJson.driverId + 1);\r",
											"});\r",
											"\r",
											"pm.test(\"Postman Collection Variable 'testDriverId' is set to the driverId from the response body.\", () => {\r",
											"\r",
											"    pm.expect(responseJson.driverId).to.equal(pm.collectionVariables.get(\"testDriverId\"));\r",
											"});\r",
											""
										],
										"type": "text/javascript",
										"packages": {},
										"requests": {}
									}
								}
